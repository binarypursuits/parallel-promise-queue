{
  "name": "parallel-promise-queue",
  "description": "Provides a single function to handle a parallel promise queue with ability to dictate concurrency.",
  "version": "1.0.3",
  "main": "dist/index.js",
  "scripts": {
    "pretest": "npm run lint",
    "test:js": "mocha tests/**/*.spec.js",
    "test:ts": "mocha -r ts-node/register tests/**/*.spec.ts --recursive",
    "test": "npm run test:js && npm run test:ts",
    "test:coverage": "nyc npm run test",
    "lint": "tslint ./src/**/*.ts",
    "prebuild": "npm run lint",
    "build": "webpack --progress"
  },
  "author": "Brian Bolli <brianbolli@gmail.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/brian-bolli/parallel-promise-queue.git"
  },
  "license": "MIT",
  "private": false,
  "devDependencies": {
    "@types/chai": "^4.2.12",
    "@types/mocha": "^8.0.2",
    "@types/node": "^10.17.28",
    "chai": "^4.2.0",
    "coveralls": "^3.1.0",
    "mocha": "^8.1.1",
    "nyc": "^15.1.0",
    "ts-loader": "^8.0.2",
    "ts-node": "^9.0.0",
    "tslint": "^6.1.3",
    "typescript": "^3.9.7",
    "webpack": "^4.44.1",
    "webpack-cli": "^3.3.12"
  }
}
